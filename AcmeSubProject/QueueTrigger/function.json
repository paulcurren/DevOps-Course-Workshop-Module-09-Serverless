{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "msg",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "acmesub-translations-queue",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "message",
      "type": "table",
      "tableName": "AcmeTranslations",
      "rowKey": "{rowKey}",
      "partitionKey": "message",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ]
}